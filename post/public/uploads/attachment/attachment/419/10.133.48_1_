<script>
   $(document).ready(function(){
		 $('#login').click(function(){
			var  username=$("#username").val();
    		var  password=$("#pwd").val();
	    	$.ajax({
				url:'/admins/login',
				type:'post',
				dataType:'json',
				timeout:1000,
				data:{'username':username,'password':password},
				success:function(data){
						if (data!=0) 
						self.location.href="/seek/"+data;  
						else
						  $('#loginys').text("登录失败，请检查用户名或密码");
						  }
				});
			 
			 });
		 $('#cancel').click(function(){
			 $("#username").val('');
			 $("#pwd").val('');
			 });
			 
     });
</script>
   <div id="myModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
      <div class="modal-header">	    <h3>管理员登录</h3>		</div>
	  <div class="modal-body"> 
	  <p>
			<label>用户名:</label><input type="text" size:10 id="username">
				<label>密  码:</label><input type="password" size:10 id="pwd"><br/>
	            <button id='login' type="button" class="btn">登录</button>
	  <button class="btn" id='cancel' data-dismiss="modal" aria-hidden="true">取消</button>
	  </p>
	  <div class="modal-footer" id="loginys"></div>
   </div>
</body>
